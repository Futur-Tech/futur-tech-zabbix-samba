            <discovery_rules>
                <discovery_rule>
                    <name>Samba DC: Inbound AD replications discovery</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>samba4cmd[&quot;getErrorFrom&quot;]</key>
                    <delay>15m</delay>
                    <lifetime>1h</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Samba DC: inbound AD replication {#DSA} {#NC_DN}</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>samba4cmd[getFromErrorDetails,{#NC_DN},{#DSA_OBJECTGUID}]</key>
                            <applications>
                                <application>
                                    <name>Samba</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>var myArray = JSON.parse(value);
for (var i = 0; i &lt; myArray .length; i++){
  var obj = myArray[i];
  var risultato = &quot;&quot;;
  for (var key in obj){
    var value = obj[key];
    var n = key.endsWith(&quot;CONSECUTIVE_FAILURES}&quot;);
    if (n){
      risultato = value;
    }
  }
}
return risultato</params>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;0</expression>
                                    <name>Check Samba DC inbound AD replication {#DSA} {#NC_DN}</name>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
